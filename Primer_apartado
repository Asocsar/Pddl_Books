POSIBLE
(:action leer_antes
  :parameters (?b1 - book ?b2 - book ?m - month)
  :precondition (and (preced ?b1 ?b2) (not (exists (?bn - book) (and (preced ?b1 ?bn) (preced ?bn ?b2) ) ) )  (or (preced_ready ?b1) (not_preced ?b1)) (not (done ?b2) ) (not (done ?b1) ) (not (forbide_month ?b1 ?m)))
  :effect (and (done ?b1) (assign ?b1 ?m) (preced_ready ?b2) (forbide_month ?b2 ?m) )
  )
)
